    "r=0\n",
    "l=0\n",
    "split=0\n",
    "for i in s:\n",
    "    if(i==\"R\"):\n",
    "        r+=1\n",
    "    elif(i==\"L\"):\n",
    "        l+=1\n",
    "        \n",
    "    if(r==l):\n",
    "        split+=1\n",
    "        r=0\n",
    "        l=0\n",
    "print(split)"
   ]
