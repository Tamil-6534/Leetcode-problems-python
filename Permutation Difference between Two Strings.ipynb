    "t = \"edbac\"\n",
    "dit={}\n",
    "for i in s:\n",
    "    dit[i]=s.index(i)\n",
    "lst=[]\n",
    "for j in range(len(s)):\n",
    "    diff = dit[s[j]]-dit[t[j]]\n",
    "    absolute=abs(diff)\n",
    "    lst.append(absolute)\n",
    "sum=0\n",
    "for k in lst:\n",
    "    sum+=k\n",
    "print(sum)"
