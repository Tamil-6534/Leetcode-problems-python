    "\n",
    "values=[]\n",
    "def ping(t):\n",
    "    rangee=t-3000\n",
    "    values.append(t)\n",
    "\n",
    "    while(values[0]<rangee):\n",
    "        values.pop(0)\n",
    "    return len(values)\n",
    "    \n",
    "\n",
